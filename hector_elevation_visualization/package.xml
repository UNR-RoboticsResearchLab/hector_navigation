<package>
  <name>hector_elevation_visualization</name>
  <version>1.0.0</version>
  <description>hector_elevation_visualization is an tool for visualization of elevation maps generated by the hector_elevation_mapping package</description>
  <maintainer email="test@test.edu">Thorsten Graber</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/hector_elevation_visualization</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Thorsten Graber</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>visualization_msgs</build_depend>
  <build_depend>hector_elevation_msgs</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>hector_map_tools</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>cmake_modules</build_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>visualization_msgs</run_depend>
  <run_depend>hector_elevation_msgs</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>hector_map_tools</run_depend>
  <run_depend>dynamic_reconfigure</run_depend>
  <run_depend>cmake_modules</run_depend>

  <!-- Dependencies needed only for running tests. -->
  <!-- <test_depend>visualization_msgs</test_depend> -->
  <!-- <test_depend>hector_elevation_msgs</test_depend> -->
  <!-- <test_depend>tf</test_depend> -->
  <!-- <test_depend>hector_map_tools</test_depend> -->
  <!-- <test_depend>dynamic_reconfigure</test_depend> -->
  <!-- <test_depend>cmake_modules</test_depend> -->

</package>
